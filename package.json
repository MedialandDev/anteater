{
  "name": "ml.anteater",
  "version": "1.2.1",
  "description": "Medialand Interactive libs",
  "main": "",
  "scripts": {
    "start": "npm run storybook",
    "build": "cross-env NODE_ENV=production webpack --config ./internal/webpack/webpack.prod.js",
    "plop": "plop --plopfile ./internal/generator/index.js",
    "storybook": "npm run test && start-storybook -p 9001 -c ./internal/storybook",
    "storybook-build": "build-storybook -c ./internal/storybook -o dist/storybook",
    "lint": "eslint src/**/*.{js,vue}",
    "lint-staged": "lint-staged",
    "flow": "flow",
    "test": "jest --json --outputFile=.jest-test-results.json"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": "lint"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "ml.anteater/(.*)$": "<rootDir>/src/$1",
      "^vue$": "vue/dist/vue.common.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MedialandDev/anteater.git"
  },
  "author": "Medialand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MedialandDev/anteater/issues"
  },
  "homepage": "https://github.com/MedialandDev/anteater#readme",
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.4",
    "@storybook/addon-centered": "^4.0.4",
    "@storybook/addon-jest": "^4.0.4",
    "@storybook/addon-knobs": "^4.0.4",
    "@storybook/addon-links": "^4.0.4",
    "@storybook/addon-options": "^4.0.4",
    "@storybook/addon-storysource": "^4.0.4",
    "@storybook/addon-viewport": "^4.0.4",
    "@storybook/vue": "^4.0.4",
    "@vue/server-test-utils": "^1.0.0-beta.25",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "babel-plugin-jsx-vue-functional": "^2.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-pug-html": "^0.1.2",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue-libs": "^3.0.0",
    "flow-bin": "^0.86.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "plop": "^2.1.0",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "storybook-addon-jest": "0.0.1",
    "storybook-readme": "^4.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.17",
    "vue-jest": "^3.0.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "rxjs": "^5.5.6"
  }
}
