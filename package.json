{
  "name": "ml.anteater",
  "version": "1.1.3",
  "description": "Medialand Interactive libs",
  "main": "",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --config ./internal/webpack/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./internal/webpack/webpack.prod.js",
    "build-dev": "cross-env NODE_ENV=development webpack --config ./internal/webpack/webpack.prod.js",
    "plop": "plop --plopfile ./internal/generator/index.js",
    "storybook": "npm run test && start-storybook -p 9001 -c ./internal/storybook",
    "storybook-build": "build-storybook -c ./internal/storybook -o dist/storybook",
    "lint": "eslint src/**/*.{js,vue}",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "flow": "flow",
    "test": "jest --json --outputFile=.jest-test-results.json"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": "lint"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "ml.anteater/(.*)$": "<rootDir>/src/$1",
      "^vue$": "vue/dist/vue.common.js"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "mapCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MedialandDev/anteater.git"
  },
  "author": "Medialand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MedialandDev/anteater/issues"
  },
  "homepage": "https://github.com/MedialandDev/anteater#readme",
  "devDependencies": {
    "@storybook/addon-centered": "^3.3.11",
    "@storybook/addon-knobs": "^3.3.11",
    "@storybook/addon-options": "^3.3.11",
    "@storybook/addon-viewport": "^3.3.11",
    "@storybook/vue": "^3.3.11",
    "avoriaz": "^6.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "babel-plugin-jsx-vue-functional": "^2.1.0",
    "babel-plugin-transform-pug-html": "^0.1.2",
    "babel-plugin-transform-vue-jsx": "^3.5.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue-libs": "^2.1.0",
    "flow-bin": "^0.64.0",
    "husky": "^0.14.3",
    "jest": "^22.1.2",
    "lint-staged": "^6.1.0",
    "plop": "^1.9.1",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "storybook-addon-jest": "0.0.1",
    "storybook-readme": "^3.2.0",
    "vue": "^2.5.13",
    "vue-jest": "^2.0.1",
    "vue-loader": "^14.0.2",
    "vue-template-compiler": "^2.5.13",
    "vuex": "^3.0.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "rxjs": "^5.5.6"
  }
}
